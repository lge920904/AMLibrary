<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.giveangel.amlibrary.adpublisher">

    <application android:allowBackup="true" android:label="@string/app_name">


        <service
            android:name="com.giveangel.amlibrary.adpublisher.ADPublisherService"
            android:enabled="true"
            android:exported="true">
            <intent-filter >
                <action android:name="com.example.leegaeul.adpublisher.ADPUBLISHERSERVICE" />
            </intent-filter>
        </service>
        <receiver
            android:name="com.giveangel.amlibrary.adpublisher.ADPublisherBR"
            android:enabled="true"
            android:exported="true">
            <intent-filter android:priority="99999">
                <action android:name="android.intent.action.NEW_OUTGOING_CALL"/>
                <action android:name="android.intent.action.PHONE_STATE"/>
            </intent-filter>
        </receiver>
    </application>

</manifest>
